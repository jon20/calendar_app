<%provide(:title, "Edit")%>
<h1>Edit Schedule</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@tmp, url: calendar_path) do |f| %>
      <%= f.label :title%><br>
      <%= f.text_field :title, class: "form-control"%>

      <%= f.label :text%><br>
      <%= f.text_field :body, rows: "10"%><br>
      <%= f.submit "Save", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
