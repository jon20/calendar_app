<%provide(:title, "Edit")%>
<h1>Edit Schedule</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@tmp) do |f| %>
      <%= f.label :title%>
      <%= f.text_field :title, class: "form-control"%>

      <%= f.label :text%>
      <%= f.text_field :body, rows: "10"%>
    <% end %>
  </div>
</div>
